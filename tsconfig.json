{
  "compilerOptions": {
    "target": "es5",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "lib": [
        "es2015",
        "es5",
        "es6",
        "dom"
    ],
    "noUnusedLocals": true,
    "module": "commonjs",
    "strict": false
},
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.json", "src/**/*.svg", "src/*.d.ts", "src/**/*.png"],
  "exclude": ["node_modules"]
}